{
  "name": "Cats_of_Chaouen",
  "version": "1.0.0",
  "description": "This is a mobile first web app intended to serve as the home page for the non-profit organization \"Cats of Chaouen\" based in Chefchaouen, Morocco.",
  "main": "./client/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
